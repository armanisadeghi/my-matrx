{
  "name": "my-matrx",
  "version": "1.0.0",
  "description": "Next.js app for dynamic HTML pages with Supabase",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate-uuid": "node -e \"console.log('New page ID:', require('crypto').randomUUID())\"",
    "env:pull": "doppler secrets download --project my-matrx --config dev --no-file --format env > .env.local",
    "env:push": "doppler secrets upload --project my-matrx --config dev .env.local"
  },
  "keywords": [
    "static",
    "html",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "."
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.3",
    "formidable": "^3.5.4",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "node-html-parser": "^7.0.2",
    "nodemailer": "^7.0.13",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sharp": "^0.34.5"
  }
}
